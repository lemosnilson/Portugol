algoritmo "MOD E DIV"

var
   x, y: inteiro
   a, b, c: real

inicio
      x <- 10
      y <- 3
      a <- x/y
      escreval(a)
      b <- x DIV y //retorna a parte inteira da divisão
      escreval(b)
      c <- x MOD y // retorna o resto da divisão
      escreval(c)

fimalgoritmo
